{
	"name": "CLB",
	"description": "Builds all possible customized locators",	
    "version": "1.0.1",
    "devtools_page": "clb_devtools.html",	
    "manifest_version": 2,
	"content_scripts": [{
			"matches": ["<all_urls>"],
			"js": ["clb_content.js"],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	]
}